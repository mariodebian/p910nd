#!/usr/bin/make -f
# -*- makefile -*-
#export DH_VERBOSE=1


override_dh_auto_clean:
	rm -f changelog
	dh_auto_clean

override_dh_auto_build:
	$(MAKE) USE_WRAP=1 LOCKFILE_DIR=\"/var/log\"
	# upstream don't provive changelog file, generate from p910nd.c header
	head -88 p910nd.c | tail -71 | sed -e 's/ \*//g' > changelog

override_dh_installinit:
	dh_installinit --update-rcd-params='start 20 2 3 4 5 . stop 20 0 1 6 .'

%:
	dh $@

